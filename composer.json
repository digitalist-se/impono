{
    "name": "wkse/open-source-tag-manager",
    "license": "AGPLv3",
    "type": "project",
    "description": "Open source tag manager",
    "autoload": {
        "psr-4": {
            "": "src/",
            "Features\\": "behat/Features/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.5.13",
        "doctrine/dbal": "2.5.13",
        "doctrine/doctrine-bundle": "~1.8",
        "twig/extensions": "~1.5",
        "symfony/assetic-bundle": "~2.8",
        "symfony/acl-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "~2.6",
        "symfony/monolog-bundle": "~3.1",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~3.1",
        "incenteev/composer-parameter-handler": "~2.1",
        "friendsofsymfony/rest-bundle": "^2.0",
        "jms/serializer-bundle": "~2.3",
        "doctrine/doctrine-fixtures-bundle": "~2.4",
        "nelmio/api-doc-bundle": "^2.13",
        "friendsofsymfony/user-bundle": "~2.0",
        "jms/metadata": "~1.6",
        "friendsofsymfony/oauth-server-bundle": "1.5.2",
        "stof/doctrine-extensions-bundle" : "1.2.*",
        "myclabs/deep-copy" : "~1.7",
        "friendsofsymfony/http-cache-bundle" : "~1.3",
        "matthiasmullie/minify": "~1.3",
        "doctrine/migrations": "1.4.*",
        "doctrine/doctrine-migrations-bundle": "1.2.*",
        "knplabs/knp-gaufrette-bundle" : "0.4.*",
        "aws/aws-sdk-php" : "2.8.*",
        "league/flysystem" : "1.0.*",
        "guzzlehttp/guzzle": "~6.3.0",
        "symfony/process" : "3.4.*",
        "sentry/sentry-symfony": "^1.0",
        "sonata-project/notification-bundle": "^3.2",
        "liip/monitor-bundle": "^2.5",
        "symfony/dom-crawler" : "3.4.*",
        "php-coveralls/php-coveralls": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "liip/functional-test-bundle": "~1.8",
        "phploc/phploc": "~3.0",
        "pdepend/pdepend" : "2.0.*",
        "phpmd/phpmd": "2.2.*",
        "sebastian/phpcpd": "2.0.*",
        "squizlabs/php_codesniffer" : "2.3.*",
        "behat/behat" : "~3.2",
        "behat/symfony2-extension" : "2.1.*",
        "behat/mink-extension" : "2.3.*",
        "behat/mink-browserkit-driver" : "1.3.*",
        "fabpot/goutte" : "3.2.*",
        "symfony/var-dumper" : "2.6.*",
        "codeception/codeception" : "~2.2",
        "coduo/php-matcher" : "2.1.*",
        "behat/mink-selenium2-driver": "^1.2",
        "satooshi/php-coveralls": "dev-master",
        "symfony/web-server-bundle": "^3.4",
        "mayflower/php-codebrowser": "^1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "php": "5.6.31"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
